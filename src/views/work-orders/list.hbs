<div class="container p-4">
    <div class="row">
      <div class="mx-auto">
          <div class="card">
              <div class="card-body">
                  <form action="/work-orders/filter" method="get">
                    <div class="form-group">
                    </div>
                  </form>
              </div>
          </div>
      </div>
    </div>
    <div class="row">
      <div class="mx-auto">
          <div class="card">
              <div class="card-body">
                  <table class="table">
                      <thead>
                          <th>ID</th>
                      </thead>
                      <tbody>
                          {{#each workOrders}}
                          <tr>
                              <td>{{id}}</td>
                          </tr>
                          {{else}}
                          {{/each}}
                      </tbody>
                  </table>
              </div>
          </div>
      </div>  
    </div>
</div>