<div class="container p-4">
    <div class="row">
      <div class="mx-auto">
          <div class="card">
              <div class="card-body">
                  <form action="/work-orders/filter" method="get">
                    <div class="form-group">
                    </div>
                  </form>
              </div>
          </div>
      </div>
    </div>
    <div class="row">
      <div class="mx-auto">
          <div class="card">
              <div class="card-body">
                  <table class="table">
                      <thead>
                          <th>Nro Orden</th>
                          <th>Cliente:</th>
                          <th>Vehículo:</th>
                          <th>Teléfono:</th>
                          <th>Fecha Ingreso:</th>
                          <th>Encargado:</th>
                          <th>Estado</th>
                          <th>Descripcion</th>
                      </thead>
                      <tbody>
                          {{#each workOrders}}
                          <tr>
                              <td>{{id}}</td>
                              <td>{{cliente}}</td>
                              <td>{{vehiculo}}</td>
                              <td>{{telefono}}</td>
                              <td>{{created_at}}</td>
                              <td>{{encargado}}</td>
                              <td>{{status}}</td>
                              <td>{{description}}</td>
                          </tr>
                          {{else}}
                          {{/each}}
                      </tbody>
                  </table>
              </div>
          </div>
      </div>  
    </div>
</div>